



<% @passenger.each do |passenger|%>

  <p>Name: <%= link_to passenger.name, passenger_path(passenger) %> Phone Number: <%=passenger.phone_num%> </p>

  <%= button_to "Edit", edit_passenger_path(passenger), method: :get %>

  <%= button_to "Delete", passenger, method: :delete, data: {confirm: "Are you sure you want to delete this passenger?"} %>

<%end %>